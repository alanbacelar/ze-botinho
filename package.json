{
  "name": "ze-botinho",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "lambda:create": "claudia create --name=ze-botinho --handler lambda.handler --region us-east-1",
    "lambda:update": "claudia update",
    "lambda:generate-proxy": "claudia generate-serverless-express-proxy --express-module app"
  },
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "axios": "^0.17.1",
    "body-parser": "~1.17.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "watson-developer-cloud": "^2.42.0"
  },
  "devDependencies": {
    "claudia": "^2.14.2"
  }
}
